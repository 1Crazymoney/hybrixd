{
  "symbol":"dgb",
  "name":"digibyte",
  "module":"quartz",
  "mode":"digibyte",
  "user":"placeholder_user",
  "pass":"placeholder_password",
  "factor":8,
  "fee":0.0001,
  "host":["http://127.0.0.1:14022/"],
  "cache":20000,
  "throttle":0.1,
  "pass": "d8d21ccfe2fa",
  "block-explorer-host": "https://chainz.cryptoid.info/dgb/api.dws",

  "quartz":{
    "init" : [
      "curl('asset://$symbol','','POST',{jsonrpc:'1.0',id:Math.floor(Math.random()*10000),method:'getinfo',params:[]},{'content-type':'text/plain'})",
      "logs(1,'module $symbol: '+(data?'connected':'failed connection')+' to host [$host]')"
    ],
    "status" : [
      "curl('asset://$symbol','','POST',{jsonrpc:'1.0',id:Math.floor(Math.random()*10000),method:'getinfo',params:[]},{'content-type':'text/plain'})",
      "logs(1,'module $symbol: '+(data?'connected':'failed connection')+' to host [$host]')"
    ],
    "unspent" : [
      "logs(1, 'testoejfoiejfiowjef')",
      "func('blockexplorer','exec',{command:['$symbol','unspent','$1','$2']})"
    ],
     "balance" : [
      "test('$1',2,1,'$1')",
      "stop(1,'No source address')",
      "curl('$block-explorer-host','?key=$pass&q=getbalance&a=$1', 'GET', '',{Accept:'application/json','Content-Type':'application/json'})",
      "test(data,2,1,data)",
      "stop(1,'No result!')",
      "time($cache,data)",
      "pass(data/Math.pow(10,$factor))"
    ],
    "push" : [
      "curl('asset://$symbol','','POST',{jsonrpc:'1.0',id: Math.floor(Math.random()*10000),method:'sendrawtransaction',params:['$1']},{'content-type':'text/plain'})",
      "test(data,2,1,data)",
      "stop(1,'Broadcast failed!')",
      "tran('.result',data,2,1)",
      "stop(1,'No result!')",
      "pass(data)"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [],
    "contract" : [],
    "fee" : [
      "pass('$fee')"
    ]
  }
}
