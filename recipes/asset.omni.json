{
  "symbol":"omni",
  "name":"Omni",
  "module":"quartz",
  "mode":"bitcoinjslib.omni",
  "factor":8,
  "fee":0.00004946,
  "fee-symbol":"btc",
  "keygen-base":"btc",
  "contract":"1",
  "host":"https://www.omniwallet.org",
  "blockexplorer":"https://api.omniexplorer.info",
  "cache":180000,
  "throttle":0.1,
  "quartz":{
    "init" : [
      "stop(1,'Not yet implemented!')"
    ],
    "status" : [
      "stop(1,'Not yet implemented!')"
    ],
    "unspent" : [
      "func('blockexplorer','exec',{target:{symbol:'$fee-symbol'}, command:['unspent','$1','$fee']})",
      "pass({'unspents':data.unspents,'change':String(Number(data.change)+0.00000546)})"
    ],
    "balance" : [
      "time(120000)",
      "curl('$blockexplorer','/v1/address/addr?addr=$1','POST','addr=$1',{'content-type':'application/x-www-form-urlencoded'})",
      "tran('.balance',data,2,1)",
      "stop(1,'Bad result!')",
      "find({'id':'$contract'},data,1,2)",
      "tran({value:'.value',in:'.pendingpos',out:'.pendingneg'},data,2,1)",
      "next(0,{value:'0',in:'0',out:'0'})",
      "time($cache,data)",
      "form(parseInt(data.value)+parseInt(data.in)-parseInt(data.out),$factor,'reduce')"
    ],
    "push" : [
      "stop(1,'NOT YET OPERATIONAL!')",
      "tran('.value',data,2,1)",
      "pass(0)",
      "time($cache,data)",
      "form(data,$factor,'reduce')",
      "stop(1,'Not yet implemented!')"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [
       "stop(1,'Not yet implemented!')"
    ],
    "contract" : [
       "stop(0,'$contract')"
    ],
    "fee" : [
      "pass('$fee')"
    ]
  }
}
