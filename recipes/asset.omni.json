{
  "symbol":"omni",
  "name":"Omni",
  "module":"quartz",
  "mode":"bitcoinjslib.omni",
  "factor":8,
  "fee":0.00004946,
  "fee-symbol":"btc",
  "keygen-base":"btc",
  "contract":"1",
  "host":"https://api.omniexplorer.info",
  "cache":180000,
  "throttle":0.1,
  "quartz":{
    "init" : [
      "stop(1,'Not yet implemented!')"
    ],
    "status" : [
      "time($cache)",
      "curl('asset://$symbol','/v1/address/addr?addr=1FoWyxwPXuj4C6abqwhjDWdz6D4PZgYRjA','POST','addr=1FoWyxwPXuj4C6abqwhjDWdz6D4PZgYRjA',{'content-type':'application/x-www-form-urlencoded'})",
      "test(data,1,2,data)",
      "stop(0,{module:'$symbol',synced:null,blocks:null,fee:null,supply:null,difficulty:null,testmode:null,version:null})",
      "stop(1,'No result')"

    ],
    "unspent" : [
      "func('blockexplorer','exec',{command:['$fee-symbol','unspent','$1','$fee']})",
      "pass({'unspents':data.unspents,'change':String(Number(data.change)+0.00000546)})"
    ],
    "balance" : [
      "time($cache)",
      "curl('asset://$symbol','/v1/address/addr?addr=$1','POST','addr=$1',{'content-type':'application/x-www-form-urlencoded'})",
      "tran('.balance',data,2,1)",
      "stop(1,'No result!')",
      "find({'id':'$contract'},data,1,2)",
      "tran({value:'.value',in:'.pendingpos',out:'.pendingneg'},data,2,1)",
      "next(0,{value:'0',in:'0',out:'0'})",
      "form(parseInt(data.value)+parseInt(data.in)-parseInt(data.out),$factor,'reduce')"
    ],
    "push" : [
      "call('asset://btc','push/$1')"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [
       "stop(1,'Not yet implemented!')"
    ],
    "transaction" : [
      "curl('asset://$symbol','/v1/transaction/tx/$1','GET','addr=$1',{'content-type':'application/x-www-form-urlencoded'})",
      "tran({id:'.txid',fee:'.fee',attachment:'',timestamp:'.blocktime',symbol:'$symbol','fee-symbol':'$fee-symbol',ammount:'.amount',source:'=.sendingaddress|unknown',target:'=.referenceaddress|unknown'},data,2,1)",
      "stop(1,'error')",
      "stop(0,data)"
    ],
    "contract" : [
      "stop(0,'$contract')"
    ],
    "fee" : [
      "pass('$fee')"
    ],
    "sample" : [
      "stop(0,{address:'1FoWyxwPXuj4C6abqwhjDWdz6D4PZgYRjA',transaction:'1989dc575f94dcb45485b78b1b762e13a3a6d2eb59422b6217c93218a54ff80b'})"
    ]
  }
}
