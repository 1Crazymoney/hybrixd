{
  "symbol":"omni",
  "name":"Omni",
  "module":"quartz",
  "mode":"bitcoinjslib.omni",
  "factor":8,
  "fee":0.000035,
  "fee-symbol":"btc",
  "keygen-base":"btc",
  "contract":"1",
  "host":"https://api.omniexplorer.info",
  "Xhost":"wss://www.omniwallet.org",
  "cache":120000,
  "throttle":5,
  "quartz":{
    "init" : [
       "stop(1,'Not yet implemented!')"
    ],
    "status" : [
       "stop(1,'Not yet implemented!')"
    ],
    "unspent" : [
      "func('blockexplorer','exec',{target:{symbol:'$fee-symbol'}, command:['unspent','$1','$2']})"
    ],
    "balance" : [
      "time(32000)",
      "curl('asset://$symbol','/v1/address/addr?addr=$1','POST','addr=$1',{'content-type':'application/x-www-form-urlencoded'})",
      "tran('.balance',data,2,1)",
      "stop(1,'Bad result!')",
      "find({'id':'$contract'},data,1,2)",
      "tran('.value',data,2,1)",
      "pass(0)",
      "time($cache,data)",
      "form(data,$factor,'reduce')"
    ],
    "push" : [
      "tran('.value',data,2,1)",
      "pass(0)",
      "time($cache,data)",
      "form(data,$factor,'reduce')",
       "stop(1,'Not yet implemented!')"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [
       "stop(1,'Not yet implemented!')"
    ],
    "contract" : [
       "stop(1,'Not yet implemented!')"
    ],
    "fee" : [
      "pass('$fee')"
    ]
  }
}
