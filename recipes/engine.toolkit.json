    "checkpeers_curl" : [
      "poke 'testhost',data.value",
      "time 60000,'timeout',0",
      "hook null,0",
      "curl('http://$testhost:17876/nxt','','GET',{'content-type':'text/plain'})",
      "poke('hostresponse')",
      "tran('.errorCode',1,2)",
      "done('error')",
      "peek('hostresponse')",
      "done()",
      "tran('.result',2,1)",
      "done('noresult')",
      "peek('testhost')",
      "done(data)"
    ],
    "test" : [
      "data '{some:\"yolo\",real:\"dirty\",JSON:\"works?\"}'",
      "jpar 1 2",
      "done",
      "fail 'bad JSON!'"
    ],
    "proberen" : [
      "rout /asset/eth/status/0x4d3244b0677b86d06b12cdbaa3ac4f15baaa6d5b",
      "poke currentStatus",
      "flow '$local::previousStatus' @nietsGebeurd @doeIets",
      "@nietsGebeurd",
      "done 'Er is niets gebeurd!'",
      "@doeIets",
      "data '$local::previousStatus'",
      "flow 'undefined' @eersteKeer @gaDoor",
      "@eersteKeer",
      "peek currentStatus",
      "poke local::previousStatus",
      "done 'Dit is de eerste aanroep van dit script. Status opgeslagen...'",
      "@gaDoor",
      "peek currentStatus",
      "poke local::previousStatus",
      "done 'We doen iets!!!'"
    ],
    "testeach" : [
      "time 60000",
      "each [1,2,3,4,5] 'waitalittle'",
      "done"
    ],
    "waitalittle" : [
      "time 4000 'stopped!' 0",
      "math '${.key}*1000'",
      "wait data",
      "done 'OK'"
    ]
  },
  "router":{
    "_hidden":true,
    "_help" : "(experimental!) Toolkit engine to stage new functional dependencies.",
        "test"  : {
                "_help" : "This is just a test.",
                "_ref" : {
