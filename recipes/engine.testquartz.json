{
  "engine":"testquartz",
  "name":"Quartz Testing Engine",
  "module":"quartz",
  "cache":1000,
  "throttle":10,
  "OK":"OK",
  "quartz":{
    "done" : [
      "done '$OK'"
    ],
    "fail" : [
      "fail '$OK'"
    ],
    "data" : [
      "data '$OK'",
      "done"
    ],
    "size" : [
      "data '$OK'",
      "size",
      "done"
    ],
    "call" : [
      "call 'done'",
      "done"
    ],
    "with" : [
      "data '100'",
      "poke 'testVarA'",
      "poke 'testVarB'",
      "with 'testVarA' 'hash'",
      "with 'testVarB' ['math','+100'] ['math','+50']",
      "done data+' $testVarA $testVarB'"
    ],
    "each" : [
      "each [0,1,2,3,4] 'each_test'",
      "done"
    ],
    "each_test" : [
      "done data.key+':'+data.value+':$OK'"
    ],
    "true" : [
      "data 1",
      "true '=1' @okay",
      "done 'FAIL'",
      "@okay",
      "done 'OK'"
    ],
    "flow" : [
      "data 'OK'",
      "flow {'no':1,'OK':2,'neither':1} 3",
      "done 'FAIL'",
      "done 'OK'",
      "done 'FAIL'"
    ],
    "jump" : [
    ],
    "have" : [
    ],
    "save" : [
      "data 'OK'",
      "save 'testfile'",
      "have",
      "true '=48' @okay",
      "done 'FAIL'",
      "@okay",
      "done 'OK'"
    ],
    "load" : [
      "data 'OK'",
      "save 'testfile'",
      "load 'testfile'",
      "file",
      "done"
    ],
    "push" : [
      "data ['A','B','C','D','E','F']",
      "#push '*',[-1,1,3]",
      "#push 'Yolo',[0,3,-2]",
      "push '*' 0",
      "done"
    ],
    "fuse" : [
      "data ['A','B']",
      "fuse ['C','D']",
      "done"
    ],
    "drop" : [
      "data 'testmenow'",
      "drop -1,-1",
      "done"
    ],
    "excl" : [
      "data ['a','b','c','d','c','e','f']",
      "excl ['c','b','a']",
      "done"
    ],
    "filt" : [
      "data ['a','b','c','d','c','e','f']",
      "filt ['c','b']",
      "done"
    ],
    "uniq" : [
      "data ['a','b','c','c','d','d','c','e','f','g','g','g','g','h']",
      "uniq",
      "done"
    ],
    "many" : [
      "data ['a','b','c','d','c','e','f']",
      "many",
      "done"
    ],
    "peek" : [
      "peek 'proc::1'",
      "done"
    ],
    "X" : [
      "data 'placeholder'"
    ]
  },
  "router":{
    "_hidden" : true,
    "done"  : "Test done()",
    "fail"  : "Test fail()",
    "data"  : "Test data()",
    "size"  : "Test size()",
    "call"  : "Test call()",
    "with"  : "Test with()",
    "each"  : "Test each()",
    "true"  : "Test true()",
    "flow"  : "Test flow()",
    "save"  : "Test save()",
    "load"  : "Test load()",
    "push"  : "Test push()",
    "fuse"  : "Test fuse()",
    "drop"  : "Test drop()",
    "excl"  : "Test excl()",
    "filt"  : "Test filt()",
    "uniq"  : "Test uniq()",
    "many"  : "Test many()",
    "peek"  : "Test peek()",
    "Y" : "Test ",
    "X" : "Test "
  }
}
