{
  "engine":"testquartz",
  "name":"Quartz Testing Engine",
  "module":"quartz",
  "cache":1000,
  "throttle":10,
  "OK":"OK",
  "quartz":{
    "done" : [
      "done '$OK'"
    ],
    "fail" : [
      "fail '$OK'"
    ],
    "data" : [
      "data '$OK'",
      "done"
    ],
    "call" : [
      "call 'done'",
      "done"
    ],
    "each" : [
      "each [0,1,2,3,4] 'each_test'",
      "done"
    ],
    "each_test" : [
      "done data.key+':'+data.value+':$OK'"
    ],
    "true" : [
      "data 1",
      "true '=1' @okay",
      "done 'FAIL'",
      "@okay",
      "done 'OK'"
    ],
    "flow" : [
      "data 'OK'",
      "flow {'no':1,'OK':2,'neither':1} 3",
      "done 'FAIL'",
      "done 'OK'",
      "done 'FAIL'"
    ],
    "jump" : [
    ],
    "have" : [
    ],
    "save" : [
      "data 'OK'",
      "save 'testfile'",
      "have",
      "true '=48' @okay",
      "done 'FAIL'",
      "@okay",
      "done 'OK'"
    ],
    "load" : [
      "data 'OK'",
      "save 'testfile'",
      "load 'testfile'",
      "file",
      "done"
    ],
    "X" : [
      "data 'placeholder'"
    ]
  },
  "router":{
  	"done"  : "Test done()",
  	"fail"  : "Test fail()",
  	"data"  : "Test data()",
  	"call"  : "Test call()",
  	"each"  : "Test each()",
  	"true"  : "Test true()",
  	"flow"  : "Test flow()",
  	"save"  : "Test save()",
  	"load"  : "Test load()",
  	"Y" : "Test ",
  	"X" : "Test "
  }
}
