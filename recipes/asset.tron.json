{
  "symbol":"trx",
  "name":"Tron",
  "module":"quartz",
  "mode":"tron",
  "factor":6,
  "fee":0,
  "contract": "",
  "host":["https://api.trongrid.io"],
  "cache":10000,
  "throttle":10,
  "quartz":{
    "init" : [
      "logs(1,'module $symbol: initialized')"
    ],
    "balance" : [
      "data '$1'",
      "code 'base58' 'hex'",
      "case 'lower'",
      "drop -8",
      "test('$contract'==='', @tronBalance, @tokenBalance)",
      "@tronBalance",
      "data '{address: \"$data\"}'",
      "curl 'asset://$symbol' '/walletsolidity/getaccount' 'POST'",
      "tran '.balance' @end @fail",
      "@tokenBalance",
      "pass '0'",
      "@fail",
      "pass '0'",
      "@end",
      "atom"
    ],
    "push" : [
      "data '$1'",
      "code 'hex' 'utf-8'",
      "poke 'txData'",
      "curl 'asset://$symbol' '/wallet/broadcasttransaction' 'POST'",
      "tran '.result' @end @fail",
      "@end",
      "flow 'true' 1 @fail",
      "peek 'txData'",
      "jpar",
      "tran '.txID' 1 @failDetails",
      "done",
      "@fail",
      "pass \"Couldn't make transaction\"",
      "done",
      "@failDetails",
      "pass \"Couldn't retrieve transaction ID\"",
      "done"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [
      "done"
    ],
    "contract" : [
      "pass('')"
    ],
    "fee" : [
      "pass('$fee')"
    ],
    "unspent" : [
      "pass({})"
    ],
    "history" : [
      "pass '[]",
      "done",
      "curl 'asset://$symbol' '/walletextension/gettransactionstothis' 'POST'",
      "poke 'txTo'",
      "curl 'asset://$symbol' '/walletextension/gettransactionsfromthis' 'POST'",
      "fuse 'txTo'"
    ]
  }
}
