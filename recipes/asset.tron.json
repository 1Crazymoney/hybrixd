{
  "symbol":"trx",
  "name":"Tron",
  "module":"quartz",
  "mode":"tron",
  "factor":6,
  "fee":0,
  "contract": "",
  "host":["https://api.trongrid.io"],
  "cache":10000,
  "throttle":10,
  "quartz":{
    "init" : [
      "logs(1,'module $symbol: initialized')"
    ],
    "balance" : [
      "data '$1'",
      "code 'base58' 'hex'",
      "case 'lower'",
      "drop -8",
      "test('$contract'==='', @tronBalance, @tokenBalance)",
      "@tronBalance",
      "data '{address: \"$data\"}'",
      "curl 'asset://$symbol' '/walletsolidity/getaccount' 'POST'",
      "tran '.balance' @end @fail",
      "@tokenBalance",
      "pass '0'",
      "@fail",
      "pass '0'",
      "@end",
      "atom"
    ],
    "push" : [
      "logs",
      "done",
      "data '$1'",
      "jpar",
      "curl 'asset://$symbol' 'wallet/broadcasttransaction' 'POST' '$data'",
      "test(data.error,1,2)",
      "fail(data.message)",
      "tran('.id',2,1)",
      "fail('Transaction Failed! '+jstr(data))",
      "pass(data)"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [
      "curl 'asset://$symbol' '/walletextension/gettransactionstothis' POST",
      "poke 'txTo'",
      "curl 'asset://$symbol' '/walletextension/gettransactionsfromthis' POST",
      "fuse 'txTo'"
    ],
    "contract" : [
      "pass('')"
    ],
    "fee" : [
      "pass('$fee')"
    ]
  }
}
