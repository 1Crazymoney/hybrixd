{
  "symbol":"bts",
  "name":"BitShares",
  "module":"quartz",
  "deterministic-module":"bitshares",
  "mode":"bitshares.main",
  "asset-id":"1.3.0",
  "factor":8,
  "fee":0.05,
  "contract":"undefined",
  "host":"wss://bitshares.openledger.info/ws",
  "cache":120000,
  "throttle":5,
  "quartz":{
    "init" : [
      "curl('asset://$symbol','','',{'method': 'call', 'params': [1, 'login', ['', '']], 'id': Math.floor(Math.random()*10000)})",
      "logs(1,'module $symbol: '+(data?'connected':'failed connection')+' to host [$host]')"
    ],
    "status" : [
      "stop(1,'Not yet implemented!')"
    ],
    "unspent" : [
      "curl('asset://$symbol','','',{method:'call',params:[0,'get_key_references',[['$1']]], id: Math.floor(Math.random()*10000)})",
      "pass(data)"
    ],
    "balance" : [
      "curl('asset://$symbol','','',{'id':1, 'method':'call', 'params':[0,'get_named_account_balances',['$1',['$asset-id']]], id: Math.floor(Math.random()*10000)})",
      "tran('.result[0].amount',data,2,1)",
      "stop(1,'No result!')",
      "form(data,$factor,'reduce')",
      "pass(data)"
    ],
    "push" : [
      "curl('asset://$symbol','','',{'id':1, 'method':'call', 'params':['broadcast_transaction',['$1']], id: Math.floor(Math.random()*10000)})",
      "test(data,2,1,data)",
      "stop(1,'Broadcast failed!')",
      "tran('.result',data,2,1)",
      "stop(1,'No result!')",
      "pass(data)"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "history" : [
      "stop(1,'Not yet implemented!')"
    ],
    "contract" : [
      "stop(1,'Not yet implemented!')"
    ],
    "fee" : [
      "pass('$fee')"
    ]
  }
}
