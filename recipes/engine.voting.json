{
  "engine":"voting",
  "name":"Voting engine",
  "module":"quartz",
  "cache":100,
  "throttle":10,
  "quartz":{
    "test" : [
      "data '$1'",
      "hash 'sha256'",
      "data 'storemeyo!'",
      "save 'key'",
      "load 'key'",
      "jpar 1 2",
      "done",
      "fail data"
    ],
    "listAddress" : [
      "call 'listAddresses'",
      "done"
    ],
    "listAddresses" : [
      "poke '::coreMode','addresses'",
      "#call 'coreList'",
      "done ['addressA','addressB','addressC','addressD','addressE']",
      "done"
    ],
    "addAddress" : [
      "data '$2'",
      "splt ':'",
      "poke 'userhash' data[0]",
      "poke 'useraddr' data[1]",
      "poke '::coreMode' 'addresses'",
      "call 'coreList'",
      "find '$userhash' 1 4",
      "poke '::coreMode' 'addresses'",
      "poke '::coreData' '$useraddr'",
      "call 'coreAdd'",
      "done",
      "@notelligible",
      "done 'User not elligible!'"
    ],
    "listEligibility" : [
      "poke '::coreMode','elegibility'",
      "call 'coreList'",
      "done"
    ],
    "addEligibility" : [
      "poke '::coreData','$2'",
      "poke '::coreMode','elegibility'",
      "call 'coreAdd'",
      "done"
    ],
    "coreList" : [
      "data '$::coreMode $1'",
      "hash 'sha256'",
      "hook [] 0",
      "#load '$data'",
      "#file",
      "#jpar",
      "data ['pietjePuk','aliB','harryVanBeek','someBody','andMe']",
      "done"
    ],
    "coreAdd" : [
      "done '$::coreMode $::coreData'",
      "data '$::coreMode $1'",
      "hash 'sha256'",
      "poke 'idx'",
      "data '$::coreData'",
      "splt ','",
      "poke 'inputdata'",
      "hook 4",
      "load '$idx'",
      "file",
      "jpar 2",
      "@emptylist",
      "data []",
      "@savelist",
      "push $inputdata",
      "save '$idx'",
      "done 'Added $::coreData to list $::coreMode '+'_$1'"
    ],
    "coreReset" : [
      "data '$1 $2'",
      "hash 'sha256'",
      "poke 'idx'",
      "data []",
      "save '$idx'",
      "done 'Core list reset'"
    ]
  },
  "router" : {
    "test" : {
      "_list": "Test routine",
      "_ref" : {
        "_list" : "session_id",
        "_this" : "Test routine. Needs an additional /id to function!"
      }
    },
    "listAddress" : {
      "_help": "List all eligibility addresses [alias to listAddresses]",
      "_ref" : {
        "_list" : "session_id",
        "_this" : "An array is returned listing all eligibility addresses."
      }
    },
    "listAddresses" : {
      "_help": "List all eligibility addresses",
      "_ref" : {
        "_list" : "session_id",
        "_this" : "An array is returned listing all eligibility addresses."
      }
    },
    "addAddress" : {
      "_help": "Add eligibility adresses to voting session",
      "_ref" : {
        "_list" : "session_id",
          "_ref" : {
              "_list" : "eligibility_address",
              "_this" : "Adds an eligibility address or addresses under the specified voting session ID. [format: hash:address]"
          }
      }
    },
    "listEligibility" : {
      "_help": "List all eligibility codes",
      "_ref" : {
        "_list" : "session_id",
        "_this" : "An array is returned listing all eligibility codes."
      }
    },
    "addEligibility" : {
      "_help": "Add eligibility codes to voting session",
      "_ref" : {
        "_list" : "session_id",
          "_ref" : {
              "_list" : "eligibility_code",
              "_this" : "Adds an eligibility code or codes under the specified voting session ID."
          }
      }
    },
    "coreList" : {
      "_help": "List core",
      "_ref" : {
        "_list" : "storage_id",
        "_this" : "List core storage."
      }
    },
    "coreAdd" : {
      "_help": "Add to core",
      "_ref" : {
        "_list" : "session_id",
          "_ref" : {
              "_list" : "storage_id",
              "_this" : "Add to core storage object list."
          }
      }
    },
    "coreReset" : {
      "_list": "Reset the core storage list",
      "_ref" : {
        "_list" : "voting_session_id",
        "_this" : "Empties the core session list, deleting its data contents."
      }
    }
  }
}
