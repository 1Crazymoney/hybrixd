{
  "symbol":"flo",
  "name":"Florincoin",
  "module":"quartz",
  "mode":"bitcoinjslib.florincoin",
  "addressRegex":"^\\F[a-zA-Z0-9]{33}$",
  "factor":8,
  "confirmed":6,
  "fee":0.001,
  "byteFee":0.00001024,
  "host": "https://livenet.flocha.in/api",
  "cache":15000,
  "throttle":10,
  "import":"insight",
  "quartz":{
    "sample" : [
      "done {address:'FEYWiz5brGLZQ5Wg9FS845dqb94mB1hHxe',transaction:'f9a1ce8f34d03261817b905c0d08034bc25b9fbb827c7ae58de4f724920138f5'}"
    ],
    "fee" : [
      "data '$1'",
      "flow 'undefined' 1 3",
      "data $fee",
      "jump @allDone",
      "math '$fee+($byteFee*$1)'",
      "@allDone"
    ],
    "attachment" : [
      "time $timeout",
      "call transactionData/$1",
      "tran .floData 2 1",
      "data null",
      "done"
    ],
    "message" : [
      "call attachment/$1",
      "done"
    ],
    "cron" : [
      "# empty cron to avoid getting fee, since FLO insight does not support this!",
      "done"
    ],
    "test" : [
      "done {hash:'1988B19F'}"
    ]
  }
}
