{
  "symbol":"waves",
  "name":"Waves",
  "module":"quartz",
  "mode":"waves",
  "factor":8,
  "fee":0.1,
  "host":"http://127.0.0.1:6869",
  "cache":15000,
  "throttle":5,
  "quartz":{
    "init" : [
      "curl('asset://$symbol','/utils/seed/2','GET')",
      "logs(1,'module $symbol: '+(data?'connected':'failed connection')+' to host [$host]')"
    ],
    "status" : [
      "curl('asset://$symbol','/utils/seed/2','GET')",
      "chck({module:'$symbol',synced:null,blocks:null,fee:null,supply:null,difficulty:null,testmode:null,version:null},2)",
      "stop(0,data)",
      "stop(1,'No result')"
    ],
    "factor" : [
      "stop(0,$factor)"
    ],
    "balance" : [
        "pass($1,2,1)",
        "stop(1,'No source address')",
        "curl('asset://$symbol','/balance/$1','GET')",
        "chck('.balance',2)",
        "stop(0,data)",
        "stop(1,'No result')"
    ],
    "fee" : [
      "stop(0,'$fee')"
    ],
    "push" : [
              "curl('asset://$symbol','/transactions/broadcast','POST',data)",
              "chck('.id',1,2)",
              "stop(1,'Broadcast failed!')",
              "stop(0,data)"
              ],
    "history" : [
                 "curl('asset://$symbol','/transactions/address/$1/limit/50]','GET')"
                 ],
    "contract" : [],
    "unspent" : []
  }
}
