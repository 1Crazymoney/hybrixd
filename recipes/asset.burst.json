{
  "symbol":"burst",
  "name":"Burst",
  "module":"quartz",
  "mode":"nxt.burst",
  "addressRegex":"^(BURST-)?([A-HJ-N-P-Z2-9]{4}-){3}[A-Z0-9]{5}$",
  "factor":8,
  "fee":1,
  "host":["https://wallet1.burst-team.us:2083/burst","https://wallet.burst.cryptoguru.org:8125/burst","https://wallet2.burst-team.us:2083/burst"],
  "api-documentation":"https://burstwiki.org/en/",
  "cache":15000,
  "throttle":5,
  "genesisBlockTime":1407729600,
  "import":"nrs-api",
  "quartz":{
    "balance":[
      "data '$1'",
      "regx '^BURST\\-' 2 1",
      "push 'BURST-' 0",
      "poke 'address'",
      "data '$symbol'",
      "find '.' 1 3",
      "call 'tokenBalance/$address'",
      "jump 2",
      "call 'mainBalance/$address'"
    ],
    "unspent":[
      "data '$3'",
      "regx '^BURST\\-' 2 1",
      "push 'BURST-' 0",
      "poke 'target'",
      "data '$symbol'",
      "find '.' 1 3",
      "call 'tokenUnspent/$1/$2/$target/$4'",
      "jump @done",
      "call 'mainUnspent/$1/$2/$target/$4'",
      "@done",
      "done"
    ],
    "mainUnspent":[
      "call 'fee'",
      "poke 'numFee'",
      "atom true",
      "poke 'txFee'",
      "data '$2'",
      "math '-$numFee'",
      "atom true",
      "poke 'txAmount'",
      "data 'recipient=$3&publicKey=$4&amountNQT=$txAmount&feeNQT=$txFee&deadline=300&broadcast=false'",
      "curl 'asset://$symbol' '?requestType=sendMoney' 'POST' {'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8'}",
      "poke 'result'",
      "tran '.errorDescription' 1 2",
      "fail",
      "done $result"
    ],
    "sample" : [
      "done({address:'BURST-HKML-NRG6-VBRA-2F8PS',transaction:'11439896918258012006',publicKey:'25cc2bb30ee7665737c9721090313c85176e485cd9a15495a0f3abc359d8d632'})"
    ],
    "history" : [
      "data '$1'",
      "regx '^BURST\\-' 2 1",
      "push 'BURST-' 0",
      "poke 'address'",
      "poke count $2 12",
      "poke offset $3 0",
      "with until [math,'$offset+$count']",
      "curl asset://$symbol '?requestType=getAccountTransactionIds&account=$address' GET {'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8'}",
      "tran '.transactionIds' 2 1",
      "fail 'Bad response from Burst node!'",
      "take $offset $count",
      "done"
    ],
    "test" :[
      "done {hash:dynamic}"
    ]
  }
}
