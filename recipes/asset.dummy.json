{
  "symbol":"dummy",
  "name":"Dummycoin (test coin for Internet of Coins)",
  "module":"quartz",
  "mode":"dummycoin",
  "factor":8,
  "fee":0.15,
  "host":"none",
  "cache":10000,
  "throttle":10,
  "quartz":{
    "init" : [
      "logs(1,'module $symbol: initialized')"
    ],
    "status" : [
      "stop(0,{module:'$symbol',synced:1,blocks:0,fee:$fee,supply:0,difficulty:0,testmode:0,version:0})"
    ],
    "unspent" : [
      "test('$1',2,1)",
      "stop(1,'Error: missing address!')",
      "pass([{amount:'$2',txid:'TXIDTXIDTXIDTXIDTXIDTXIDTXID',txn:1}])"
    ],
    "balance" : [
      "test('$1',2,1)",
      "stop(1,'Error: missing address!')",
      "pass('10000')"
    ],
    "push" : [
      "wait(3000)",
      "stop(0,'TXIDTXIDTXIDTXIDTXIDTXIDTXID')"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "sample" : [
      "stop(0,{address:'__dummyaddress__',transaction:'TXIDTXIDTXIDTXIDTXIDTXIDTXID'})"
    ],
    "transaction" : [
      "test('$1'==='TXIDTXIDTXIDTXIDTXIDTXIDTXID',1,2)",
      "stop(0,{id:'TXIDTXIDTXIDTXIDTXIDTXIDTXID',symbol:'$symbol','fee-symbol':'$symbol'})",
      "stop(1,'Illegal transaction id.')"
    ],
    "history" : [
      "stop(0,[])"
    ],
    "contract" : [
      "stop(1,'Not yet implemented!')"
    ],
    "fee" : [
      "pass('$fee')"
    ]
  }
}
