{
  "symbol":"btc",
  "name":"Bitcoin",
  "module":"quartz",
  "mode":"bitcoinjslib.bitcoin",
  "factor":8,
  "fee":0.000025,
  "host":["tcp://electrum2.cipig.net:10000","tcp://electrum1.cipig.net:10000"],
  "cache":60000,
  "throttle":10,
  "import":"electrum-tcp",
  "quartz":{
    "init" : [
      "curl('asset://$symbol','','',{'jsonrpc':'2.0','id':0,'method':'server.version','params':[]},{'content-type':'text/plain'})",
      "logs(1,'module $symbol: '+(data?'connected':'failed connection')+' to host [$host]')"
    ]
  }
}
