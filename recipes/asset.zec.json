{
  "symbol":"zec",
  "name":"ZCash",
  "module":"quartz",
  "mode":"zcash.livenet",
  "user":"user",
  "pass":"emFxMTJ3c3gK",
  "factor":8,
  "fee":0.0001,
  "host":["http://127.0.0.1:8232/"],
  "cache":15000,
  "throttle":5,

  "quartz":{
    "init" : [
      "curl('asset://$symbol','','POST',{jsonrpc:'1.0',id:Math.floor(Math.random()*10000),method:'getinfo',params:[]},{'content-type':'text/plain'})",
      "logs(1,'module $symbol: '+(data?'connected':'failed connection')+' to host [$host]')"
    ],
    "status" : [
      "curl('asset://$symbol','','POST',{jsonrpc:'1.0',id:Math.floor(Math.random()*10000),method:'getinfo',params:[]},{'content-type':'text/plain'})",
      "test(data,1,2)",
      "stop(0,{module:'$symbol',synced:null,blocks:null,fee:null,supply:null,difficulty:null,testmode:$mode==='zcash.livenet',version:null})",
      "stop(1,'No result')"
    ],
    "unspent" : [
      "rout('/source/blockexplorer/$symbol/unspent/$1/$2/$3')"
    ],
    "balance" : [
      "time($cache)",
      "func('blockexplorer','exec',{command:['$symbol','balance','$1']})"
    ],
    "push" : [
      "curl('asset://$symbol','','POST',{jsonrpc:'1.0',id: Math.floor(Math.random()*10000),method:'sendrawtransaction',params:['$1']},{'content-type':'text/plain'})",
      "test(data,2,1)",
      "stop(1,'Broadcast failed!')",
      "tran('.result',2,1)",
      "stop(1,'No result!')",
      "pass()"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "fee" : [
      "pass('$fee')"
    ],
    "sample" : [
      "stop(0,{address:'t1ar5X1AJ1JBeRZJY379bFQ3TtLuRK3YpbS',transaction:'cdea6a548efa88b30fbd110472a1ea4c0e5dee4cc768141dcb09a8c0f9ba4211'})"
    ]
  }
}
