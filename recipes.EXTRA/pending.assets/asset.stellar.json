{
  "symbol":"xlm",
  "name":"Stellar",
  "module":"quartz",
  "mode":"stellar",
  "factor":7,
  "fee":0.001,
  "host": "https://horizon.stellar.org",
  "rejectUnauthorized":false,
  "cache":15000,
  "throttle":3,
  "contract":"native",
  "quartz":{
    "init" : [
      "done"
    ],
    "factor" : [
      "pass('$factor')"
    ],
    "balance" : [
      "curl('$host','/accounts/$1','GET')",
      "tran('.balances',2,1)",
      "fail",
      "find({'asset_type':'$contract'})",
      "tran('[0].balance',2,1)",
      "fail",
      "pass"
    ],
    "fee" : [
      "data('$fee')"
    ],
    "push" : [
      "data '$1'",
      "repl '*' '/'",
      "data encodeURIComponent('$')",
      "curl('$host','/transactions?tx=$','POST', {})",
      "logs",
      "done"
    ],
    "contract" : [
      "pass('$contract')"
    ],
    "unspent" : [
      "curl('$host','/accounts/$1','GET')",
      "tran('.sequence',2,1)",
      "fail",
      "done"
    ],
    "history" : [
      "done []"
    ],

    "transaction" : [
      "done {a:1}"
    ],
   "sample" : [
      "pass({address:'GCEZWKCA5VLDNRLN3RPRJMRZOX3Z6G5CHCGSNFHEYVXM3XOJMDS674JZ',transaction:''})"
    ]
  }
}
