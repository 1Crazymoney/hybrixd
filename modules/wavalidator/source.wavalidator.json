{
  "source":"wavalidator",
  "name":"Wallet Address Validator",
  "module":"wavalidator",

  "quartz":{
    "validate" : [
      "func 'validate/$1/$2'",
      "done"
    ]
  },
  "router" : {
    "_hidden":true,
    "_help" : "Validate address formatting.",
    "validate" : {
      "_ref" : {
        "_list" : "symbol",
        "_sample" : "dummy",
        "_ref" : {
          "_list" : "address",
          "_sample" : "_dummyaddress_",
          "_this" : "Validate $ADDRESS for asset $SYMBOL."
        }
      }
    }
  }
}
