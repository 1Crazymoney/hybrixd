{
  "engine":"mockchain",
  "name":"Mockchain Explorer.",
  "module":"mockchain",
  "router" : {
    "balance" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "dummy",
        "_ref" : {
          "_list" : "address",
          "_sample": "123",
          "_this" : "Retrieve the balance for a given contract asset. Use the mine command to fill the balance."
        }
      }
    },
    "push" : {
      "_ref" : {
        "_list" : "transaction",
        "_this" : "Push a transaction to the mockchain."
      }
    },
    "transaction" : {
      "_ref" : {
        "_list" : "transactionid",
        "_sample": "0",
        "_this" : "Retrieve a transaction."
      }
    },
    "history" : {
      "_ref" : {
        "_list" : "symbol",
        "_sample": "dummy",
        "_ref" : {
          "_list" : "address",
          "_sample": "123",
          "_this" : "Retrieve the history for a given address."
        }
      }
    },
    "mine" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "dummy",
        "_ref" : {
          "_list" : "address",
          "_sample": "123",
          "_ref" : {
            "_list" : "amount",
            "_sample": "100",
            "_this" : "Mine an amount of contract asset and transfer to address."
          }
        }
      }
    }

  }
}
