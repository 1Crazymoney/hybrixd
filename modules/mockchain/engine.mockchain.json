{
  "engine":"mockchain",
  "name":"Mockchain Explorer.",
  "module":"mockchain",
  "quartz":{
    "mine" : ["func 'mine/$1/$2/$3'"],
    "balance" : [
      "data '$1'",
      "flow {'main':1} 3",
      "rout '/asset/mock/factor'",
      "jump 2",
      "rout '/asset/mock.$1/factor'",
      "poke 'factorValue'",
      "func 'balance/$1/$2'",
      "form '$factorValue'",
      "done"
    ],
    "history" : ["func 'history/$1/$2/$3/$4'"],
    "push" : ["func 'push/$1/$2'"],
    "transaction" : [
      "func 'transaction/$1/$2'"
    ],
    "message" : ["func 'message/$1/$2'"]
  },
  "router" : {
    "balance" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "btc",
        "_ref" : {
          "_list" : "address",
          "_sample": "123",
          "_this" : "Retrieve the balance for a given contract asset. Use the mine command to fill the balance."
        }
      }
    },
    "push" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "btc",
        "_ref" : {
          "_list" : "transaction",
          "_this" : "Push a transaction to the mockchain."
        }
      }
    },
    "message" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "btc",
        "_ref" : {
          "_list" : "transactionid",
          "_sample": "0",
          "_this" : "Retrieve a transaction message message/attachment."
        }
      }
    },
    "transaction" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "btc",
        "_ref" : {
          "_list" : "transactionid",
          "_sample": "0",
          "_this" : "Retrieve transaction details"
        }
      }
    },
    "history" : {
      "_ref" : {
        "_list" : "symbol",
        "_sample": "btc",
        "_ref" : {
          "_list" : "address",
          "_sample": "123",
          "_ref" : {
            "_list" : "length",
            "_ref" : {
              "_list" : "offset",
              "_this" : "Retrieve the history for a given address."
            }
          }
        }
      }
    },
    "mine" : {
      "_ref" : {
        "_list" : "contract",
        "_sample": "btc",
        "_ref" : {
          "_list" : "address",
          "_sample": "123",
          "_ref" : {
            "_list" : "amount",
            "_sample": "100",
            "_this" : "Mine an amount of contract asset and transfer to address."
          }
        }
      }
    }

  }
}
