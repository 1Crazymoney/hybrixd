{
  "name": "hybrix-web-wallet",
  "version": "1.0.7",
  "description": "web-wallet as a client for hybrixd",
  "main": "index.js",
  "scripts": {
    "start": "npm i && npm run build",
    "setup": "sh scripts/npm/setup.sh 'public'",
    "setup:dev": "sh scripts/npm/setup.sh 'dev'",
    "compile": "sh scripts/npm/compile.sh",
    "diag": "sh scripts/npm/diag.sh",
    "dist": "sh scripts/npm/dist.sh",
    "test": "clear && node ./node_modules/jest/bin/jest.js --detectOpenHandles headless-testing/main.test.js",
    "build": "sh scripts/npm/compile.sh; sh scripts/npm/dist.sh",
    "check": "sh scripts/npm/check.sh",
    "clean:build": "rm -rf dist; sh scripts/npm/compile.sh; sh scripts/npm/dist.sh",
    "lint": "sh ../common/scripts/lint/lint.sh web-wallet",
    "clean": "rm -rf dist/*"
  },
  "author": "hybrix",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-all-curried": "^4.0.0",
    "babel-plugin-curry-all": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-environment-webdriver": "^0.2.0",
    "node": "^10.10.0",
    "node-sass": "^4.9.3",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "add-module-exports-webpack-plugin": "^0.1.0",
    "babel-preset-env": "^1.7.0",
    "bowser": "^2.0.0-beta.3",
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.7.3",
    "node-fetch": "^2.2.1",
    "qr-scanner": "^1.1.1",
    "ramda": "^0.25.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "whatwg-fetch": "^2.0.4"
  },
  "sideEffects": false
}
